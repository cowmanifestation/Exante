<table border="0" cellpadding="0" cellspacing="10" class="list">
    <% @events.each_with_index do |event,i| %>
        <tr <%= 'class="alt"' if i%2==1 %>>
        <td><a href="/event/<%= event.id %>"></a><%= event.title %></td>
        <td><a href="/event/<%= event.id %>/delete">delete</a></td>
        </tr>
    <% end %>
</table>
